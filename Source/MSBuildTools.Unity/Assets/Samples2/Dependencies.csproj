<!-- This is the "special" single top level project whose purpose is to pull in all dependencies from the project/package dependency graph. -->
<!-- The generator would generate this file if it is missing (e.g. the first time), but never overwrite it after that, so the user is free to edit the project by adding NuGet packages (top level dependencies), etc. -->

<Project>

  <!-- Import the props generated by the generator for this project. -->
  <Import Project=".MSBuildForUnity/Dependencies.csproj.g.props" />

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0</TargetFrameworks>
  </PropertyGroup>

  <!-- This is an example of a PackageReference that could be added by the user. -->
  <ItemGroup>
    <PackageReference Include="Stateless" Version="4.2.1" />
  </ItemGroup>

  <!-- Import the targets generated by the generator for this projet. -->
  <Import Project=".MSBuildForUnity/Dependencies.csproj.g.targets" />

</Project>
